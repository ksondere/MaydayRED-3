#nav-arrow 
    display none
    position fixed
    top 0px
    right 0px
    z-index 99999

#main-nav 
    width 200px
    list-style none
    position fixed
    right 0px
    height 100%
    margin-top 0px
    padding-right 0px
    padding-left 20px
    z-index 9999

#main-nav li 
    margin 0px
    text-align right
    margin-top 10px
    margin-bottom 10px
    line-height 28px

.nav-link 
    margin 2px
    color #fff !important
    font-family Matchbook
    font-size 30px
    text-decoration none
    background #8B0000
    padding 4px 10px
    border-bottom solid 4px #c0c0c0

.nav-linkhover, .nav-linkfocus 
    text-decoration none !important
    font-size 35px
    background #8D0000

